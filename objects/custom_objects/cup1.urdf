<?xml version="1.0"?>
<robot name="glass_shelf_with_qr">

  <link name="base_link">
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.2"/>
      </geometry>
      <material name=" gray glass">
        <color rgba="1.0 1.0 1.0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.05" length="0.2"/>
      </geometry>
    </collision>
  </link>

  <joint name="fixed_joint" type="fixed">
    <parent link="base_link"/>
    <child link="top_link"/>
    <origin rpy="0 0 0" xyz="0 0 0.1"/>
  </joint>

  <link name="top_link">
    <visual>
      <geometry>
        <cylinder radius="0.04" length="0.01"/>
      </geometry>
      <material name="glass">
        <color rgba="1.0 1.0 1.0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.04" length="0.01"/>
      </geometry>
    </collision>
  </link>

</robot>